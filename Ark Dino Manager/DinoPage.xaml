<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Ark_Dino_Manager.DinoPage">




    <ScrollView>
        <VerticalStackLayout
            Padding="30,0"
            Spacing="25">
            <Image
                Source="dino.png"
                HeightRequest="155"
                Aspect="AspectFit"
                SemanticProperties.Description="dinos" />

            <Label
                x:Name="Label1"
                Text="Loading..."
                Style="{StaticResource Headline}"
                SemanticProperties.HeadingLevel="Level1" />



        </VerticalStackLayout>
    </ScrollView>




</ContentPage>