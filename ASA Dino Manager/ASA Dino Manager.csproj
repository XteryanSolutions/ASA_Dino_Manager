<Project Sdk="Microsoft.NET.Sdk">

	
	<PropertyGroup>
		<GenerateSatelliteAssemblies>false</GenerateSatelliteAssemblies>
	</PropertyGroup>	
	
	
	<PropertyGroup>
		<!-- Frameworks -->
		<TargetFrameworks Condition="$([MSBuild]::IsOSPlatform('windows'))">$(TargetFrameworks);net9.0-windows10.0.19041.0</TargetFrameworks>
		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'windows'">10.0.17763.0</SupportedOSPlatformVersion>
		<TargetPlatformMinVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'windows'">10.0.17763.0</TargetPlatformMinVersion>

		<!-- Output -->
		<OutputType>Exe</OutputType>
		<RootNamespace>ASA_Dino_Manager</RootNamespace>
		<UseMaui>true</UseMaui>
		<SingleProject>true</SingleProject>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<IsPackable>false</IsPackable>

		<!-- Display name -->
		<ApplicationTitle>ASA Dino Manager</ApplicationTitle>

		<!-- App Identifier -->
		<ApplicationId>com.xteryansolutions.asadinomanager</ApplicationId>

		<!-- Installer Generation -->
		<GenerateAppInstallerFile>False</GenerateAppInstallerFile>

		
		<SignAssembly>False</SignAssembly>
		<PackageIcon>dino.png</PackageIcon>
		<RepositoryType>git</RepositoryType>
		<WindowsPackageType>None</WindowsPackageType>
		<BaseOutputPath></BaseOutputPath>	
	</PropertyGroup>


	<!-- Dont know what theese are but they were in old file -->
	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net9.0-windows10.0.19041.0|AnyCPU'">
		<ApplicationVersion></ApplicationVersion>
		<ApplicationDisplayVersion></ApplicationDisplayVersion>
		<Optimize>False</Optimize>
	</PropertyGroup>

	
	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|net9.0-windows10.0.19041.0|AnyCPU'">
		<ApplicationVersion></ApplicationVersion>
		<ApplicationDisplayVersion></ApplicationDisplayVersion>
	</PropertyGroup>
	

	<!-- my custom resource -->
	<ItemGroup>
		<MauiImage Include="Resources\Images\*" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="Resources\Images\dino.png" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="Resources\Images\dino400.png" />
	</ItemGroup>
	
	
	<!-- something costura added -->
	<ItemGroup>
		<PackageReference Include="Costura.Fody" Version="6.0.0">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Microsoft.Maui.Controls" Version="$(MauiVersion)" />
		<PackageReference Include="Microsoft.Extensions.Logging.Debug" Version="9.0.0" />
	</ItemGroup>


	<ItemGroup>
		<MauiXaml Update="ArchivePage.xaml">
			<Generator>MSBuild:Compile</Generator>
		</MauiXaml>
		<MauiXaml Update="DinoPage.xaml">
			<Generator>MSBuild:Compile</Generator>
		</MauiXaml>
	</ItemGroup>

	<ItemGroup>
		<Compile Update="ArchivePage.xaml.cs">
			<DependentUpon>ArchivePage.xaml</DependentUpon>
		</Compile>
		<Compile Update="DinoPage.xaml.cs">
			<DependentUpon>DinoPage.xaml</DependentUpon>
		</Compile>
	</ItemGroup>


	<ItemGroup>
		<None Update="Properties\Resources.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>Resources.Designer.cs</LastGenOutput>
		</None>
	</ItemGroup>

</Project>